in=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"content-wrapper\">\n            <div class=\"row align-items-center\">\n                <div class=\"col-12 col-lg-{{imageWidth}}\">\n                    <div class=\"h-product-gallery\">\n            <!-- Büyük resim başlangıcı -->\n                        <div class=\"h-product-image\">\n                            <div class=\"h-product-image-item\">\n                                <input id=\"hProductImg1\" type=\"radio\" name=\"hProductImg\" checked>\n                                <img src=\"@PROJECT_PATH@/assets/images/olcer1.svg\" alt>\n                            </div>\n                            <div class=\"h-product-image-item\">\n                                <input id=\"hProductImg2\" type=\"radio\" name=\"hProductImg\">\n                                <img src=\"@PROJECT_PATH@/assets/images/olcer2.svg\" alt>\n                            </div>\n                            <div class=\"h-product-image-item\">\n                                <input id=\"hProductImg3\" type=\"radio\" name=\"hProductImg\">\n                                <img src=\"@PROJECT_PATH@/assets/images/olcer3.svg\" alt>\n                            </div> \n                            <div class=\"h-product-image-item\">\n                                <input id=\"hProductImg4\" type=\"radio\" name=\"hProductImg\">\n                                <img src=\"@PROJECT_PATH@/assets/images/olcer4.svg\" alt>\n                            </div> \n                            <div class=\"h-product-image-item\">\n      